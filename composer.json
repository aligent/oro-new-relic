{
  "name": "aligent/oro-new-relic",
  "description": "A Bundle for Oro to provide extra information to New Relic.",
  "type": "library",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Adam Hall",
      "email": "adam.hall@aligent.com.au"
    }
  ],
  "repositories": {
    "oro": {
      "type": "composer",
      "url": "https://packagist.orocrm.com"
    }
  },
  "autoload": {
    "psr-4": {
      "Aligent\\NewRelicBundle\\": "./src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "~8.0.0",
    "oro/platform": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.7",
    "phpmd/phpmd": "^2.12",
    "friendsofphp/php-cs-fixer": "~2.18.2 || ~3.1.0",
    "nelmio/alice": "~3.8.0 || ~3.9.0 || ~3.10.0",
    "theofidry/alice-data-fixtures": "~1.4.0 || ~1.5.0",
    "symfony/phpunit-bridge": "~4.4.24 || ~6.1.0",
    "squizlabs/php_codesniffer": "^3.6"
  }
}
